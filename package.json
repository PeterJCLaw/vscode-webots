{
  "name": "vscode-webots",
  "displayName": "vscode-webots",
  "description": "Webots code editing support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "webots-vrml",
        "aliases": [
          "Webots VRML",
          "Webots",
          "webots"
        ],
        "extensions": [
          ".wbt",
          ".proto"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "webots-vrml",
        "scopeName": "source.webots-vrml",
        "path": "./syntaxes/webots-vrml.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "javascript",
          "meta.embedded.block.lua": "lua"
        }
      }
    ]
  }
}
